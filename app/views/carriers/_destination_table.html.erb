<div class="panel panel-default">
  <div class="panel-heading"> <h2 class="text-center"> Flights to <%= city %> </h2>  </div>


  <table class="table sortable">
    <tr>
      <th>Carrier</th>
      <th>Flights</th>
    </tr>
    <% d_hash.each do |id, flights| %>
      <tr>
        <td> <%= link_to Carrier.find(id).name, Carrier.find(id) %> </td>
        <td> <%= flights %> </td>
      </tr>
    <% end %>
  </table>
</div>
